<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Files</title>
</head>
<body>
  <%- include("./partials/navbar") %>
  <h1>All Files</h1>

  <% if (locals.files) {%>
    <% files.forEach((file) => { %>
      <div>
        <p>File: <%= file.id %></p>
        <img src="/uploads/<%= file.name%>" alt="Some image">
      </div>
    <% }) %>
  <% } %>
</body>
</html>